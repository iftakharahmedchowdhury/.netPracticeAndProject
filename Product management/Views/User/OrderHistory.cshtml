
@model IEnumerable<Product_management.EF.OrderDetail>

<h2>Order History</h2>

<table>
    <tr>
        <th>Order Number</th>
        <th>Order Status</th>
    
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.OrderId</td>
            <td>@order.OrderStatus</td>
        </tr>
    }
</table>

